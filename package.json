{
  "name": "sistema-orcamentos",
  "version": "1.0.0",
  "description": "Sistema de orçamentos com módulos de vendas e administração",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "test": "jest",
    "test:watch": "jest --watch",
    "electron": "electron .",
    "electron-dev": "electron . --dev",
    "build": "electron-builder",
    "build-win": "electron-builder --win",
    "dist": "electron-builder --publish=never"
  },
  "keywords": [
    "orçamentos",
    "vendas",
    "administração",
    "api",
    "nodejs"
  ],
  "author": "Maycon",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "electron": "^38.2.1",
    "electron-builder": "^26.0.12",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "build": {
    "appId": "com.finiti.gerador-orcamentos",
    "productName": "Gerador de Orçamentos - Finiti",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html",
      "style.css",
      "js/**/*",
      "logo.png",
      "logo2.png",
      "orçamento.png",
      "jspdf.umd.min.js",
      "jspdf.plugin.autotable.min.js",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "logo.png",
      "target": "portable",
      "requestedExecutionLevel": "asInvoker"
    },
    "mac": {
      "icon": "logo.png",
      "target": "dmg"
    },
    "linux": {
      "icon": "logo.png",
      "target": "AppImage"
    }
  }
}
